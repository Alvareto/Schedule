@{
    ViewData["Title"] = "myCalendar";
    //  { new KeyValuePair<string, object>("events", ViewData["events"]) }
}
<h2>@ViewData["Title"]</h2>
<p>
    <a asp-controller="Requests" asp-action="Create">Switch Request</a>
</p>
@section Breadcrumbs
